{
  "name": "dendra-web-ui",
  "version": "2.0.5",
  "description": "Dendra public facing web site and application.",
  "author": "J. Scott Smith",
  "private": true,
  "scripts": {
    "babel-workers": "babel src/workers -d src/static/workers --presets=@nuxt/babel-preset-app",
    "dev2": "cross-env NODE_ENV=development nodemon src/server/index.js --watch src/server",
    "dev": "nuxt --spa",
    "dev-edge": "cross-env API_PATH=/v2 API_URI=https://api.edge.dendra.science nuxt --spa",
    "build": "nuxt build",
    "start": "nuxt start --spa",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@casl/ability": "^3.0.2",
    "@dendra-science/console-logger": "0.0.1",
    "@feathersjs/authentication-client": "^1.0.11",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio-client": "^1.2.1",
    "ajv": "^6.10.0",
    "chroma-js": "^2.0.4",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "feathers-hooks-common": "^4.20.7",
    "feathers-vuex": "git+https://github.com/DendraScience/feathers-vuex.git#jss-hotfix",
    "highcharts": "^7.1.2",
    "load-google-maps-api": "^1.3.3",
    "lodash": "^4.17.11",
    "mathjs": "^5.10.3",
    "moment": "^2.24.0",
    "nuxt": "^2.8.1",
    "socket.io-client": "^2.2.0",
    "vee-validate": "^2.2.10",
    "vuetify": "^1.5.14",
    "vuetify-loader": "^1.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "worker-loader": "git+https://github.com/DendraScience/worker-loader.git#jss-hotfix"
  }
}
