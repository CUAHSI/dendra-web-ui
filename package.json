{
  "name": "dendra-web-ui",
  "version": "2.0.6",
  "description": "Dendra public facing web site and application.",
  "author": "J. Scott Smith",
  "private": true,
  "scripts": {
    "babel-workers": "babel src/workers -d src/static/workers --presets=@nuxt/babel-preset-app",
    "dev2": "cross-env NODE_ENV=development nodemon src/server/index.js --watch src/server",
    "dev": "nuxt",
    "dev-edge": "cross-env API_PATH=/v2 API_URI=https://api.edge.dendra.science nuxt",
    "dev-preview": "cross-env API_PATH=/v2 API_URI=http://api.preview.dendra.science nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@dendra-science/console-logger": "0.0.1",
    "@feathersjs/authentication-client": "^1.0.11",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio-client": "^1.2.1",
    "@nuxtjs/vuetify": "^1.11.3",
    "ajv": "^6.12.6",
    "express": "^4.17.1",
    "feathers-hooks-common": "^4.20.7",
    "feathers-vuex": "git+https://github.com/DendraScience/feathers-vuex.git#jss-hotfix",
    "highcharts": "^9.2.2",
    "load-google-maps-api": "^2.0.2",
    "lodash": "^4.17.21",
    "mathjs": "^6.6.5",
    "moment": "^2.29.1",
    "nuxt": "^2.15.8",
    "socket.io-client": "^2.4.0",
    "validator": "^13.6.0",
    "vee-validate": "^3.4.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@mdi/js": "^5.9.55",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@vue/test-utils": "^1.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fsevents": "^2.3.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "sass": "~1.32.13",
    "worker-loader": "git+https://github.com/DendraScience/worker-loader.git#jss-hotfix"
  }
}
